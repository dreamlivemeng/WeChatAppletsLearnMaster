<!--pages/mine/mine.wxml-->


<view class='page'>
  <view class='page__hd'>
    <view class="userinfo">
      <view class="userinfo-avatar">
        <open-data type="userAvatarUrl"></open-data>
      </view>
      <view class='black-style'>
        <open-data type="userNickName"></open-data>
      </view>
    </view>
  </view>
  <view class='page__hd'>
    <view class='weui-cells'>
      <view class='weui-cell' bindtap='media_method'>
        <view class='weui-cell__bd'>媒体</view>
      </view>
      <view class='weui-cell' bindtap='files_method'>
        <view class='weui-cell__bd'>文件</view>
      </view>
      <view class='weui-cell' bindtap='storage_method'>
        <view class='weui-cell__bd'>数据缓存</view>
      </view>
      <view class='weui-cell' bindtap='location_method'>
        <view class='weui-cell__bd'>位置</view>
      </view>
      <view class='weui-cell' bindtap='devices_method'>
        <view class='weui-cell__bd'>设备</view>
      </view>
      <view class='weui-cell' bindtap='ui_method'>
        <view class='weui-cell__bd'>界面</view>
      </view>
      <view class='weui-cell' bindtap='Component_method'>
        <view class='weui-cell__bd'>组件</view>
      </view>

    </view>
  </view>
</view>