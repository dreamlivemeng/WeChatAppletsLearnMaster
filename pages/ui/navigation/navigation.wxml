<!--pages/ui/navigation/navigation.wxml-->
<view class='page'>
  <view class='page__hd'>
</view>
<view class='page__hd'>
  <view class='weui-cells'>
    <view class='weui-cell' bindtap='navigateTo_method'>
      <view class='weui-cell__bd'>navigateTo</view>
    </view>
    <view class='weui-cell' bindtap='redirectTo_method'>
      <view class='weui-cell__bd'>redirectTo</view>
    </view>
    <view class='weui-cell' bindtap='switchTab_method'>
      <view class='weui-cell__bd'>switchTab</view>
    </view>
    <view class='weui-cell' bindtap='navigateBack_method'>
      <view class='weui-cell__bd'>navigateBack</view>
    </view>
    <view class='weui-cell' bindtap='reLaunch_method'>
      <view class='weui-cell__bd'>reLaunch</view>
    </view>
  </view>
</view>
</view>