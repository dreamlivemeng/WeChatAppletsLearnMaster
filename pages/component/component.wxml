<!--pages/component/component.wxml-->

<view class='page'>
  <view class='page__hd'>
    <view class="userinfo">
      <view class="userinfo-avatar">
        <open-data type="userAvatarUrl"></open-data>
      </view>
      <view class='black-style'>
        <open-data type="userNickName"></open-data>
      </view>
    </view>
  </view>
  <view class='page__hd'>
    <view class='weui-cells'>
      <view class='weui-cell' bindtap='media_method'>
        <view class='weui-cell__bd'>视图容器</view>
      </view>
      <view class='weui-cell' bindtap='files_method'>
        <view class='weui-cell__bd'>基础内容</view>
      </view>
      <view class='weui-cell' bindtap='storage_method'>
        <view class='weui-cell__bd'>表单组件</view>
      </view>
      <view class='weui-cell' bindtap='location_method'>
        <view class='weui-cell__bd'>导航</view>
      </view>
      <view class='weui-cell' bindtap='devices_method'>
        <view class='weui-cell__bd'>媒体组件</view>
      </view>
      <view class='weui-cell' bindtap='ui_method'>
        <view class='weui-cell__bd'>地图</view>
      </view>
      <view class='weui-cell' bindtap='Component_method'>
        <view class='weui-cell__bd'>画布</view>
      </view>
      <view class='weui-cell' bindtap='Component_method'>
        <view class='weui-cell__bd'>开放能力</view>
      </view>
    </view>
  </view>
</view>